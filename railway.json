{
  "version": 2,
  "name": "web3-landing",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "about.html",
      "use": "@vercel/static"
    },
    {
      "src": "services.html",
      "use": "@vercel/static"
    },
    {
      "src": "blog.html",
      "use": "@vercel/static"
    },
    {
      "src": "faq.html",
      "use": "@vercel/static"
    },
    {
      "src": "privacy.html",
      "use": "@vercel/static"
    },
    {
      "src": "terms.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/about/?",
      "dest": "/about.html"
    },
    {
      "src": "/services/?",
      "dest": "/services.html"
    },
    {
      "src": "/blog/?",
      "dest": "/blog.html"
    },
    {
      "src": "/faq/?",
      "dest": "/faq.html"
    },
    {
      "src": "/privacy/?",
      "dest": "/privacy.html"
    },
    {
      "src": "/terms/?",
      "dest": "/terms.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
